{
	"name": "detrak",
	"private": true,
	"version": "1.1.4",
	"type": "module",
	"scripts": {
		"dev": "VITE_APP_VERSION=$(jq -r '.version' package.json) vite",
		"build": "npm run lint && tsc && VITE_DOMAIN=$DOMAIN VITE_SENTRY_DSN=$SENTRY_DSN VITE_SENTRY_ENVIRONMENT=$SENTRY_ENVIRONMENT VITE_APP_VERSION=$(jq -r '.version' package.json) vite build",
		"lint": "eslint --report-unused-disable-directives --max-warnings 0 src",
		"preview": "vite preview",
		"pwa-asset-generator": "npx pwa-asset-generator --opaque false --icon-only --favicon --padding '0' --type png res/dices.svg public && npx pwa-asset-generator --background '#0f1c2e' --padding '20%' --type png res/dices.svg public",
		"eslint:print-active-rules": "eslint --print-config src/index.ts",
		"prettier": "prettier -w ."
	},
	"dependencies": {
		"@fontsource/caveat": "^5.0.3",
		"@fontsource/inter": "^5.0.3",
		"@radix-ui/react-checkbox": "^1.0.4",
		"@radix-ui/react-dialog": "^1.0.4",
		"@radix-ui/react-dropdown-menu": "^2.0.5",
		"@radix-ui/react-hover-card": "^1.0.6",
		"@radix-ui/react-scroll-area": "^1.0.4",
		"@radix-ui/react-slot": "^1.0.2",
		"@sentry/react": "^7.61.0",
		"@sentry/vite-plugin": "^2.5.0",
		"class-variance-authority": "^0.6.1",
		"clsx": "^1.2.1",
		"i18next": "^23.2.6",
		"lucide-react": "^0.257.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.0.1",
		"tailwind-merge": "^1.13.2",
		"tailwindcss-animate": "^1.0.6"
	},
	"devDependencies": {
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vitejs/plugin-react": "^4.0.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-i18next": "^6.0.3",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"tailwindcss": "^3.3.2",
		"typescript": "^5.0.2",
		"vite": "^4.3.9",
		"vite-plugin-pwa": "^0.16.4"
	}
}
